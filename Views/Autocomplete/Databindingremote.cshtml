@section SampleHeading{
    <span class="sampleName">Autocomplete / Data Binding - Remote data</span>}
@section ControlsSection{
    <div class="frame">
        <div class="control">
            @{
                Html.EJ()
                    .Autocomplete("searchCustomer")
                    .Datasource(d => d.URL("//js.syncfusion.com/ejServices/wcf/NorthWind.svc/").Offline(false).CrossDomain(true))
                    .Query("ej.Query().from('Suppliers').select('SupplierID', 'ContactName')")
                    .AutocompleteFields(f => f.Text("ContactName").Key("SupplierID"))
                    .Width("100%")
                    .WatermarkText("Search a customer")
                    .Render();
            }
        </div>
    </div>
}
