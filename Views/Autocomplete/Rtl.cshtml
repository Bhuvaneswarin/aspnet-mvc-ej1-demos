@section SampleHeading{
    <span class="sampleName">Autocomplete / Rtl</span>}
@section ControlsSection{
    <div class="frame">
        <div class="control">
            @{
                Html.EJ()
                    .Autocomplete("selectCar")
                    .Width("100%")
                    .Datasource((IEnumerable<CarsList>)ViewBag.datasource)
                    .EnableRTL(true)
                    .WatermarkText("Select a car")
                    .Render();
            }
        </div>
    </div>
}
